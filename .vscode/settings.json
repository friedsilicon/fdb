// Place your settings in this file to overwrite default and user settings.
{
    "cmake.buildDirectory": "${workspaceRoot}/../vsc-build",
    "cmake.installPrefix": "${workspaceRoot}/../vsc-install",
    "cmake.generator": "Ninja",
    "cmake.configureEnvironment": {
        "CPPUTEST_DIR": "/usr/local/Cellar/cpputest/3.8/"
    },
    "cmake.configureArgs": [
       "-DWITH_COVERAGE=ON"
    ],
    "files.associations": {
        "cstdlib": "cpp",
        "fdb_log.h": "c",
        "fdb_private.h": "c",
        "string": "c",
        "string_view": "c",
        "vector": "c",
        "algorithm": "cpp",
        "locale": "c",
        "typeinfo": "cpp",
        "skiplist.h": "c",
        "fdb_hexdump.h": "c"
    }
}